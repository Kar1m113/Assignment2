{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
 "width": 722,
  "height":288,
  "data": {"url": "https://raw.githubusercontent.com/Kar1m113/Assignment2/main/OccupationTest2.csv"},
  "layer": [
    {
      "mark": "bar",
      "encoding": {
        "x": {"field": "JobTitle"},
        "y": {"aggregate": "sum","field":"Value"},
        "color":{"field":"Group"},
        "tooltip":[
          {"field":"JobTitle","title":"Income Range"},
          {"field":"Value","title":"Number of Migrants"},
          {"field":"Group","title":"Gender"}
        ]
      }
    },
    {
      "mark":"rule",
      "encoding": {
        "y": {"aggregate": "median", "field": "Value","title":"Number of Migrants/Asylum Seekers"},
        "color": {"value": "Black"},
        "size": {"value": 5}
      }
    }
  ]
}
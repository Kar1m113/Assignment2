{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/Kar1m113/Assignment2/main/OccupationTest.csv"},
  "mark": "bar",
  "encoding": {
    "x": {"field": "JobTitle"},
    "y": {"aggregate": "sum", "field": "Value"},
    "color": {"field": "Group"},
    "tooltip":[
          {"field":"Value", "type": "quantitative","title":"Number of Workers:"},
          {"field":"Group","type":"nominal","title":"Gender:"},
          {"field":"JobTitle","title":"Occupation:"}
      ]
  }
}